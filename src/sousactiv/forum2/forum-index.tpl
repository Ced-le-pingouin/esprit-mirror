<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Frameset//EN"
   "http://www.w3.org/TR/REC-html40/frameset.dtd">
<html>
<head>
<meta http-equiv="content-type" content="text/html; charset=utf-8">
<title>{html.title}</title>
<script type="text/javascript" language="javascript">
<!--
var g_iIdNiveau = {g_iIdNiveau};
var g_iTypeNiveau = {g_iTypeNiveau};
var g_iIdEquipe = 0;
function oFrmForums() { return top.frames["FORUMS"]; }
function oFrmSujets() { return top.frames["SUJETS"]; }
function oFrmListeSujets() { return top.frames["SUJETS"].frames["SUJETS_LISTE"]; }
function oFrmInfosSujet() { return top.frames["SUJET_INFOS"]; }
function oFrmMessages() { return top.frames["MESSAGES"]; }
function oFrmMessagesMenu() { return top.frames["MESSAGES_MENU"]; }
//-->
</script>
<head>
<frameset rows="65,195,60,*,21,23" border="0" frameborder="0" framespacing="0">
<frame name="TITRE" src="{frame['titre'].src}" frameborder="0" marginwidth="0" marginheight="0" scrolling="no" noresize="noresize">
<frame name="SUJETS" src="{frame['sujets'].src}" frameborder="0" marginwidth="0" marginheight="0" scrolling="no" noresize="noresize">
<frame name="SUJET_INFOS" src="" frameborder="0" marginwidth="0" marginheight="0" scrolling="auto" noresize="noresize">
<frame name="MESSAGES" src="" frameborder="0" marginwidth="10" marginheight="10" scrolling="auto" noresize="noresize">
<frame name="MESSAGES_MENU" src="" frameborder="0" marginwidth="0" marginheight="0" scrolling="no" noresize="noresize">
<frame name="MENU" src="{frame['menu'].src}" frameborder="0" marginwidth="0" marginheight="0" scrolling="no" noresize="noresize">
</frameset>
</html>
